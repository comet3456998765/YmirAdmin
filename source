						log('[ ‚úÖ ]: Disabled loop bringing '..plrName(plr))
					else log('[ ‚ùå ]: You are not loop bringing '..plrName(plr)) end
				else log('[ ‚ùå ]: You are not loop bringing '..plrName(plr)) end
			else log('[ ‚ùå ]: No player found') end
		else 
			if Bringing then
				Bringing = nil
				log('[ ‚úÖ ]: Disabled loop bringing')
			else log('[ ‚ùå ]: You are not loop bringing anyone') end
		end
